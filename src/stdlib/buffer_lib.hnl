
@nif __inc_buffer_lib__

@def __inc_buffer_lib__

use collections_lib
use foundation
use status_lib

struct Buffer
    buffer: Collection
    _idx: int
end

fn from_collection(collection)
    let buff = new Buffer()

    buff.buffer = collection
    buff._idx = 0

    return buff
end

fn read(buffer, num_bytes)
end

@end
